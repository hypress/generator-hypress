{
  "name": "generator-hypress",
  "version": "0.0.3",
  "description": "Yeoman Generator for hypress",
  "homepage": "",
  "author": {
    "name": "Cem Derin",
    "email": "cemyderin@gmail.com",
    "url": ""
  },
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "keywords": [
    "yeoman-generator"
  ],
  "devDependencies": {
    "adm-zip": "^0.4.13",
    "coveralls": "^3.0.7",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-config-xo": "^0.27.2",
    "eslint-plugin-prettier": "^3.1.1",
    "husky": "^3.0.9",
    "jest": "^24.9.0",
    "lint-staged": "^9.4.2",
    "ncp": "^2.0.0",
    "prettier": "^1.18.2",
    "request": "^2.88.0",
    "yeoman-assert": "^3.1.1",
    "yeoman-test": "^2.0.0"
  },
  "engines": {
    "npm": ">= 8.9.0"
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "git-user-email": "^0.2.2",
    "git-user-name": "^2.0.0",
    "randomstring": "^1.1.5",
    "slugify": "^1.3.5",
    "yeoman-generator": "^4.2.0",
    "yosay": "^2.0.2"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "printWidth": 90
        }
      ]
    },
    "plugins": [
      "prettier"
    ]
  },
  "scripts": {
    "pretest": "eslint .",
    "precommit": "lint-staged",
    "test": "jest",
    "update-skeleton": "node ./__dev/update-skeleton.js"
  },
  "repository": "https://cemderin@github.com/hypress/generator-hypress.git",
  "license": "MIT"
}
